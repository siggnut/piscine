int    ft_strcmp(char *s1, char *s2)
{
    int    i;

    i = 0;
    while (s1[i] != '\0')
    {
        if (s1[i] != s2[i])
        {
            return (s1[i] - s2[i]);
        }
        i++;
    }
    return (0);
}
#include <stdio.h>

int main() {
  char s1[] = "hello";
  char s2[] = "hewwo";
  int first;
  first = ft_strcmp(s1, s2);
  printf("%d", first);
  return 0;
}
