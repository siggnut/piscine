int test(char *str)
{
  unsigned int i;
  i = 0;
  //int size_of;
 //size_of = 0;
 //char str1;
  //str1 = 0;
  while (str[i]) //same as str[i] != '\0'
  {
    //str1 = i;
    i++;
    }
    return (i);
  }
  int main()
  {
    char str[] = "hello";
    test(str);
   //printf("%d", str); doesnt work, because..? int not stored somewhere?
   printf("%d", test(str)); //works because returns i?
    }
