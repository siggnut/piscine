#include <stdlib.h>

int	ft_strlen(char *str)
{
	int	i;

	i = 0;
	while (str[i])
	{
		i++;
	}
	return (i);
}

int	ft_ultimate_range(int **range, int min, int max)
{
	int	i;
	int	size;

	if (min >= max)
	{
		*range = NULL;
		return (0);
	}
	size = max - min;
	*range = (int *)malloc(sizeof(range) * size);
	if (!*range)
		return (-1);
	i = 0;
	while (min < max)
	{
		(*range)[i] = min;
		min++;
		i++;
	}
	return (i);
}

#include <stdio.h>
int main()
{
	int min = -10;
	int max = 10;
	int i = 0;
	int *range;
	int elements = ft_ultimate_range(&range, min, max);
	while (i < elements)
	{
		printf("%d ", range[i]);
		i++;
	}
}
