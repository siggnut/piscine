int ft_iterative_factorial(int nb)
{
    int result;
    int i;

    result = 1;
    i = 2;
    if (nb < 0)
    {
        return(0);
    }
    while(i <= nb)
    {
        result *= i;
        i++;
    }
    return (result);
}
#include <stdio.h>
int main()
{
    int nb = 5;
    printf("%d\n", ft_iterative_factorial(nb));
}
